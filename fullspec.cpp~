sensceInterval(double d){// 1<d< (?)2
    int i;
    double t,lt,dt;    
    lt=0;
    t=1/d;
    for(int i=0;i<100;i++,lt=t,t+=1./d){
            while(t>1)
                t-=1;
          dt=t-lt;
          if(dt>0){
              
          
          }
        
   }


}
